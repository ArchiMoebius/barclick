; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="BarClick"
config/description="Can you remember all the patterns?"
run/main_scene="res://Main.tscn"
boot_splash/image="res://icons/android-chrome-512x512.png"
config/icon="res://icon.png"

[display]

window/size/width=480
window/size/height=720
window/size/borderless=true
window/size/always_on_top=true
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[gdnative]

singletons=[  ]

[physics]

common/enable_pause_aware_picking=true
2d/default_gravity=0

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/subsurface_scattering/quality=2
environment/default_clear_color=Color( 0.0901961, 0.0941176, 0.180392, 1 )
